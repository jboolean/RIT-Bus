							<xs:element name="Sundays" type="xs:boolean" minOccurs="0" />
							<xs:element name="Weekdays" type="xs:boolean" minOccurs="0" />
							<xs:element name="Exception" type="xs:date" minOccurs="0" />
							<xs:element name="Addition" type="xs:date" minOccurs="0" />
							<xs:element name="Holidays" type="xs:boolean" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Destinations">
					<xs:complexType>
					</xs:complexType>
				</xs:element>
				<xs:element name="Timetable">
					<xs:complexType>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
